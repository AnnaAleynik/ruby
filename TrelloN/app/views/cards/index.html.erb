<h2>Your cards</h2>
<% @cards.each do |card| %>
	<h3><%= card.title %></h3>
	<%= link_to 'Show', card_path(card) %>
	<%= link_to 'Edit', edit_card_path(card) %>
	<%= link_to 'Destroy', card_path(card), method: :delete, data: { confirm: 'Are you sure?'} %>
	<hr>
<% end %>
<%= link_to 'New card', new_card_path %>